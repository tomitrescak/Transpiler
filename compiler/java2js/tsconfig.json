    "../java2ts/config/SourceMap.ts",
    "../java2ts/index.ts",
    "../java2ts/tests/access_checks_test.ts",
    "../java2ts/tests/classes_test.ts",
    "../java2ts/tests/enum_test.ts",
    "../java2ts/tests/expressions_test.ts",
    "../java2ts/tests/field_declarations_test.ts",
    "../java2ts/tests/function_parameters_test.ts",
    "../java2ts/tests/functions_test.ts",
    "../java2ts/tests/member_access_test.ts",
    "../java2ts/tests/modifiers_test.ts",
    "../java2ts/tests/multiple_files_test.ts",
    "../java2ts/tests/source_maps_test.ts",
    "../java2ts/tests/statements_check_test.ts",
    "../java2ts/tests/statements_test.ts",
    "../java2ts/tests/type_check_functions.ts",
    "../java2ts/tests/type_check_test.ts",
    "../java2ts/tests/type_delarations.test.ts",
    "../java2ts/tests/type_parameters_test.ts",
    "../java2ts/visitors/BlockVisitor.ts",
    "../java2ts/visitors/CompilationBundleVisitor.ts",
    "../java2ts/visitors/CompilationUnitVisitor.ts",
    "../java2ts/visitors/EnumDeclarationVisitor.ts",
